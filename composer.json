{
    "name": "micky/mkycore",
    "description": "Micky framework core",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mickael Ndinga",
            "email": "ndingamickael@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.5",
        "ext-pdo": "*",
        "nesbot/carbon": "^2.63",
        "psr/http-message": "^1.0",
        "psr/container": "^2.0",
        "http-interop/response-sender": "^1.0",
        "psr/http-server-handler": "^1.0",
        "middlewares/whoops": "^2.0",
        "ext-readline": "*",
        "twig/twig": "^3.4",
        "league/flysystem": "^3.11",
        "fakerphp/faker": "^1.21"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpunit/php-code-coverage": "^9.2",
        "symfony/var-dumper": "^6.1"
    },
    "autoload": {
        "psr-4": {
            "MkyCore\\": "src/"
        },
        "files": [
            "src/Helpers/helpers.php",
            "src/Migration/Config/Database/Config.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "MkyCore\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    }
}
