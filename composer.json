{
    "name": "micky/framework",
    "description": "Micky framework core",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mickael Ndinga",
            "email": "ndingamickael@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4",
        "psr/log": "^1|^2|^3",
        "symfony/dotenv": "^5.3",
        "ext-json": "*",
        "ext-pdo": "*",
        "symfony/yaml": "^5.4",
        "ext-simplexml": "*",
        "guzzlehttp/guzzle": "^7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.9",
        "phpunit/php-code-coverage": "^9.2",
        "symfony/var-dumper": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "MkyCore\\": "core/",
            "DebugBar\\": "core/DebugBar/"
        },
        "files": [
            "core/Helpers/config.php",
            "core/Helpers/route.php",
            "core/Helpers/view.php",
            "core/Helpers/lang.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "MkyCore\\Tests\\": "tests/"
        }
    }
}
